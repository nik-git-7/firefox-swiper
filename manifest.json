{

  "manifest_version": 2,
  "name": "Swipe",
  "version": "1.0",

  "description": "Go back and forward with two finger swipe on the trackpad",

  "icons": {
    "48": "icons/swipe-48.png",
    "96": "icons/swipe-96.png"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["swipe.js"]
    }
  ]

}